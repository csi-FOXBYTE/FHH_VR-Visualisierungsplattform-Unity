#MenuBar_ViewModeGroup {
    flex-direction: row;
    align-items: center;
    justify-content: center;
    width: 64px;
    margin: 0px;
    margin-right: 10px;
    border-radius: 16px;
    padding: 0px;
    background-color: var(--app-color-quaternary);
}


.menu-bar__viewModeGroupButton {
    margin: 0px;
    align-self: center;
    align-content: flex-start;
    align-items: flex-start;
    justify-content: center;
    height: 32px;
    width: 32px;
    padding: 0px;
    border-radius: 16px;
    background-color: var(--app-color-primary-contrast);
}

.menu-bar__viewModeGroupButton:hover {
    background-color: var(--app-color-primary);
    border-radius: 8px;
    --unity-image-tint-color: var(--app-color-primary-contrast);
}

.menu-bar__viewModeGroupButton:hover .menu-bar-button__icon {
    --unity-image-tint-color: var(--app-color-primary-contrast);
}

